            "email": "philip.weinke@digitalnoise.de"
        }
    ],
    "require": {
        "php": ">=8.1",
        "digitalnoise/command-launcher": "^0.1",
        "symfony/framework-bundle": "^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/messenger": "^6.2"
    }
}
