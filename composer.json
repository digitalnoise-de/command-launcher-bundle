{
    "name": "digitalnoise/command-launcher-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Digitalnoise\\CommandLauncherBundle\\": "src/"
        },
        "exclude-from-classmap": ["/Tests/"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Michael Ommen",
            "email": "michael.ommen@digitalnoise.de"
        },
        {
